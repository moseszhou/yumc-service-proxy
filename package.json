{
  "name": "yumc-service-proxy",
  "version": "1.0.3",
  "private": false,
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/esm/index.d.ts",
      "require": "./lib/cjs/index.js",
      "import": "./lib/esm/index.js"
    },
    "./rn": {
      "types": "./lib/esm/rn.d.ts",
      "require": "./lib/cjs/rn.js",
      "import": "./lib/esm/rn.js"
    }
  },
  "files": [
    "lib",
    "rn.js",
    "rn.d.ts"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.build.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^24.10.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}